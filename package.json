      "commit-msg": "pnpm commitlint --edit $1"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "resolutions": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "pnpm": {
    "overrides": {
      "react": "19.2.4",
      "react-dom": "19.2.4"
    },
    "ignoredBuiltDependencies": [
      "@biomejs/biome",
      "sharp"
    ]
